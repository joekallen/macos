#! /usr/bin/env bash

# DESCRIPTION
# Sets up and launches (if necessary) installed software.

# SETTINGS
set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'
SCRIPT_PATH="$MACOS_CONFIG_PATH/bin/setup_software"

# EXECUTION
if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: Software setup script does not exist or is not executable.\n"
fi
